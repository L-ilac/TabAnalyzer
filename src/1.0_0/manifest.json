{
   "background": {
      "page": "tablisteditor.html",
      "persistent": false
   },
   "chrome_url_overrides" : {
      "newtab" : "tablisteditor.html"
    },

   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Like OneTab but leaner and with minimum permissions, without tracking.",
   //"key": "",
   "manifest_version": 2,
   "name": "tabAnalyzer",
   "options_page": "options.html",
   "permissions": [ "tabs", "activeTab", "storage", "unlimitedStorage", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Tab Analyzer",
      "default_icon": {
         "16": "images/get_started16.png",
         "32": "images/get_started32.png",
         "48": "images/get_started48.png",
         "128": "images/get_started128.png"
       }
   },
   "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
   }
}
